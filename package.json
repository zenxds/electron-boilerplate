{
  "name": "electron-app",
  "version": "1.0.0",
  "description": "a electron app",
  "main": "main.js",
  "scripts": {
    "start": "concurrently \"npm run build:dev\" \"npm run start:dev\"",
    "start:dev": "cross-env ELECTRON_ENV=development DEBUG=electron WEBPACK_PORT=9005 electron .",
    "build:dev": "cross-env BABEL_ENV=development webpack-dev-server -w --port 9005 --env=development",
    "build:prod": "cross-env BABEL_ENV=production webpack --progress --env=production",
    "icons": "electron-icon-builder --input=./src/icon.png --output=.",
    "pack": "electron-builder --dir --config electron-builder.js",
    "dist": "electron-builder --config electron-builder.js",
    "prettier": "prettier src/**/*.ts src/**/*.{ts,tsx} --write",
    "test": "jest ."
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "autoUpdater": {
    "enable": true,
    "feedUrl": "http://127.0.0.1:9005/release"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn prettier && yarn test"
    }
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/src/"
    ],
    "preset": "ts-jest"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.4.3",
    "@types/debug": "^4.1.4",
    "@types/jest": "^24.0.13",
    "@types/node": "^12.0.2",
    "@types/react": "^17.0.1",
    "@types/react-css-modules": "^4.6.2",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^4.3.3",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.0",
    "antd": "^3.19.1",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-react-css-modules": "^5.2.6",
    "concurrently": "^5.3.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "electron": "^11.2.3",
    "electron-builder": "^22.9.1",
    "electron-icon-builder": "^2.0.1",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-loader": "^4.0.2",
    "file-loader": "^3.0.1",
    "husky": "^2.3.0",
    "jest": "^24.8.0",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.6.0",
    "mobx": "^5.9.4",
    "mobx-react": "^5.4.4",
    "moment": "^2.24.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-less": "^3.1.4",
    "prettier": "^1.17.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-refresh": "^0.9.0",
    "react-router-dom": "^5.0.0",
    "style-loader": "^0.23.1",
    "ts-jest": "^24.0.2",
    "typescript": "^4.1.3",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "url-loader": "^1.1.2",
    "webpack": "^4.31.0",
    "webpack-cleanup-plugin": "^0.5.1",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.11.2",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "debug": "^4.1.1",
    "electron-updater": "^4.3.5"
  }
}
